{
  "name": "mockify",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool for generating mock Shopify data using TypeScript, Faker.js, and Shopify Admin API.",
  "main": "dist/index.js",
  "bin": {
    "mockify": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "cli": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "codegen": "graphql-codegen",
    "postcodegen": "shx mv src/types/admin.types.d.ts src/types/admin.types.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@inquirer/prompts": "^7.2.3",
    "@shopify/admin-api-client": "^1.0.4",
    "axios": "^1.7.9",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "throttled-queue": "^2.1.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@shopify/api-codegen-preset": "^1.1.3",
    "@types/commander": "^2.12.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.4.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
